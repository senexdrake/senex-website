# Cache all images and favicons
/_assets/*
  Cache-Control: public, max-age=86400, immutable

# Cache immutable app assets
/_app/immutable/*
  Cache-Control: public, max-age=86400, immutable

/favicon*
  Cache-Control: public, max-age=86400, immutable

# Cache landing page for 6 hours
/
  Cache-Control: public, max-age=21600

/*.json
  Cache-Control: no-cache